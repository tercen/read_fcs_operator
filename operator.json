{
  "name": "FCS",
  "description": "Imports a Flow Cytometry Standard file (.fcs).",
  "tags": [
    "import",
    "flow cytometry"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/read_fcs_operator"
  ],
  "properties": [
    {
      "kind": "DoubleProperty",
      "name": "which.lines",
      "defaultValue": -1,
      "description": "Indicate the number of lines to be read. If -1, all rows are read, otherwise, a random sample of the size indicated by which.lines is read in"
    },
    {
      "kind": "BooleanProperty",
      "name": "gather_channels",
      "defaultValue": false,
      "description": "Whether to gather data or not."
    },
    {
      "kind": "StringProperty",
      "name": "ungather_pattern",
      "defaultValue": "time|event",
      "description": "Specify the pattern used to identify channels that won't be gathered. Case is ignored."
    },
    {
      "kind": "BooleanProperty",
      "name": "truncate_max_range",
      "defaultValue": true,
      "description": "truncate_max_range setting from read.FCS function."
    }
  ],
  "operatorSpec": {
    "kind": "OperatorSpec",
    "ontologyUri": "https://tercen.com/_ontology/tercen",
    "ontologyVersion": "0.0.1",
    "inputSpecs": [
      {
        "kind": "CrosstabSpec",
        "metaFactors": [
          {
            "kind": "MetaFactor",
            "name": "Document ID",
            "type": "string",
            "description": "Document ID(s) of FCS file(s).",
            "ontologyMapping": "document",
            "crosstabMapping": "column",
            "cardinality": "1..n",
            "factors": []
          }
        ]
      }
    ],
    "outputSpecsV2": [
      {
        "kind": "ConditionalJoinSpec",
        "description": "Output varies based on gather_channels property",
        "alternatives": [
          {
            "condition": "gather_channels = true (long/gathered format)",
            "joinSpec": {
              "kind": "OperatorJoinSpec",
              "joinOperators": [
                {
                  "kind": "JoinOperator",
                  "joinType": "left",
                  "leftPair": {
                    "kind": "ColumnPair",
                    "lColumns": [],
                    "rColumns": []
                  },
                  "rightRelation": {
                    "kind": "TableRelation",
                    "attributes": [
                      {
                        "kind": "Attribute",
                        "name": "event_id",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "channel_id",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "value",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "channel_name",
                        "type": "string"
                      },
                      {
                        "kind": "Attribute",
                        "name": "channel_description",
                        "type": "string"
                      },
                      {
                        "kind": "Attribute",
                        "name": "filename",
                        "type": "string"
                      }
                    ]
                  }
                },
                {
                  "kind": "JoinOperator",
                  "joinType": "left",
                  "leftPair": {
                    "kind": "ColumnPair",
                    "lColumns": [],
                    "rColumns": []
                  },
                  "rightRelation": {
                    "kind": "TableRelation",
                    "attributes": [
                      {
                        "kind": "Attribute",
                        "name": "filename",
                        "type": "string"
                      },
                      {
                        "kind": "Attribute",
                        "name": "mimetype",
                        "type": "string"
                      },
                      {
                        "kind": "Attribute",
                        "name": ".content",
                        "type": "string"
                      }
                    ]
                  }
                }
              ]
            }
          },
          {
            "condition": "gather_channels = false (wide format, default)",
            "joinSpec": {
              "kind": "OperatorJoinSpec",
              "joinOperators": [
                {
                  "kind": "JoinOperator",
                  "joinType": "left",
                  "leftPair": {
                    "kind": "ColumnPair",
                    "lColumns": [],
                    "rColumns": []
                  },
                  "rightRelation": {
                    "kind": "TableRelation",
                    "attributes": [
                      {
                        "kind": "Attribute",
                        "name": "event_id",
                        "type": "double"
                      },
                      {
                        "kind": "Attribute",
                        "name": "filename",
                        "type": "string"
                      }
                    ]
                  }
                },
                {
                  "kind": "JoinOperator",
                  "joinType": "left",
                  "leftPair": {
                    "kind": "ColumnPair",
                    "lColumns": [],
                    "rColumns": []
                  },
                  "rightRelation": {
                    "kind": "TableRelation",
                    "attributes": [
                      {
                        "kind": "Attribute",
                        "name": "filename",
                        "type": "string"
                      },
                      {
                        "kind": "Attribute",
                        "name": "mimetype",
                        "type": "string"
                      },
                      {
                        "kind": "Attribute",
                        "name": ".content",
                        "type": "string"
                      }
                    ]
                  }
                }
              ]
            }
          }
        ]
      }
    ]
  }
}